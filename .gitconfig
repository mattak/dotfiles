[user]
	name = mattak
	email = mattak.me@gmail.com
[alias]
	co = checkout
	st = status
	lp = log -p
	oneline = log --pretty=oneline
	ranking = shortlog -s -n --no-merges
	lg = log --graph --date=short --pretty=format:'%Cgreen%h %cd %Cblue%cn %Creset%s'
	ls = log --stat
	dm = diff master
	d1 = diff HEAD~
	d2 = diff HEAD~~
	d3 = diff HEAD~~~
	d4 = diff HEAD~~~~
	d5 = diff HEAD~~~~~
[push]
	default = tracking
